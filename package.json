{
  "name": "@reachfive/identity-core",
  "version": "1.17.1",
  "description": "Reach5 Identity Web Core SDK",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ReachFive/identity-web-core-sdk"
  },
  "author": "ReachFive",
  "main": "cjs/main.js",
  "module": "es/main.js",
  "types": "es/main/main.d.ts",
  "scripts": {
    "build": "rollup -c --environment BUILD:production",
    "watch": "rollup -c -w",
    "test": "jest",
    "watch:test": "jest --watch",
    "prepublishOnly": "npm run build",
    "tslint": "tslint --format stylish --project tsconfig.json",
    "tslint-fix": "npm run tslint -- --fix",
    "format": "prettier --write \"src/**/{*.ts,*.tsx}\""
  },
  "dependencies": {
    "buffer": "5.6.0",
    "lodash": "4.17.20",
    "winchan": "0.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@types/jest": "26.0.13",
    "@types/lodash": "4.14.161",
    "@types/webappsec-credential-management": "0.5.1",
    "core-js": "^3.6.5",
    "jest": "26.4.2",
    "jest-fetch-mock": "3.0.3",
    "prettier": "2.1.2",
    "regenerator-runtime": "^0.13.7",
    "rollup": "2.27.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-typescript2": "0.27.2",
    "rollup-plugin-uglify": "6.0.4",
    "rollup-watch": "4.3.1",
    "ts-jest": "26.3.0",
    "tslib": "2.0.1",
    "tslint": "6.1.3",
    "typescript": "4.0.2",
    "whatwg-fetch": "^3.4.1"
  },
  "files": [
    "cjs",
    "es",
    "umd"
  ],
  "jest": {
    "testURL": "http://localhost",
    "transform": {
      ".+\\.(j|t)sx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx",
      "json"
    ],
    "testMatch": [
      "**/src/**/__tests__/*.spec.ts"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/tsconfig.test.json",
        "babelConfig": false
      }
    }
  }
}
